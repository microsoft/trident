[Unit]
Description=Trident Agent
StartLimitInterval=200
StartLimitBurst=5
After=network.target network-online.target systemd-udev-settle.service trident-network.service

[Service]
Environment=RUST_LOG=info
ExecStart=/usr/bin/trident run
Restart=on-failure
RestartSec=5s

[Install]
WantedBy=multi-user.target